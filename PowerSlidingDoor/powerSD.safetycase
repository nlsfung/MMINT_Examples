  <goals id="SG1" name="Overall Goal" description="Avoid activating the actuator while the vehicle speed is greater than 15 km/h." state="True">
    <supportedBy premise="//@strategies.0"/>
    <inContextOf context="//@ASILLevels.0"/>
  </goals>
  <goals id="B1" name="Sub-Goal 1" description="The VS ECU sends the accurate vehicle speed information to the AC ECU." supports="//@strategies.0/@supportedBy.0" state="True">
    <supportedBy premise="//@solutions.0"/>
    <inContextOf context="//@ASILLevels.1"/>
  </goals>
  <goals id="B2" name="Sub-Goal 2" description="The AC ECU does not power the actuator if the vehicle speed is greater than 15 km/h." supports="//@strategies.0/@supportedBy.1" state="True">
    <supportedBy premise="//@solutions.1"/>
    <inContextOf context="//@ASILLevels.2"/>
  </goals>
  <goals id="B3" name="Sub-Goal 3" description="The VS ECU sends accurate vehicle speed information to the Redundant Switch." supports="//@strategies.0/@supportedBy.2" state="True">
