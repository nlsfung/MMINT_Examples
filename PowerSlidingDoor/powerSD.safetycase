    <inContextOf context="//@ASILLevels.3"/>
  </goals>
  <goals id="B4" name="Sub-Goal 4" description="The Redundant Switch is in an open state if the vehicle speed is greater than 15km/h." supports="//@strategies.0/@supportedBy.3" state="True">
    <supportedBy premise="//@solutions.3"/>
    <inContextOf context="//@ASILLevels.4"/>
  </goals>
  <goals id="B5" name="Sub-Goal 5" description="The actuator is actuated only when powered by the AC ECU and the Redundant Switch is closed." supports="//@strategies.0/@supportedBy.4" state="True">
    <supportedBy premise="//@solutions.4"/>
    <inContextOf context="//@ASILLevels.5"/>
  </goals>
  <goals id="B6" name="Sub-Goal 6" description="Sufficient independence of the AC ECU and the Redundant Switch is shown." supports="//@strategies.0/@supportedBy.5" state="True">
    <supportedBy premise="//@solutions.5"/>
    <inContextOf context="//@ASILLevels.6"/>
  </goals>
