  <goals id="SG1" name="Overall Goal" description="Avoid activating the actuator while the vehicle speed is greater than 15 km/h." state="True" asil="C">
    <supportedBy premise="//@strategies.0"/>
  </goals>
  <goals id="B1" name="Sub-Goal 1" description="The VS ECU sends the accurate vehicle speed information to the AC ECU." supports="//@strategies.0/@supportedBy.0" state="True" asil="C">
    <supportedBy premise="//@solutions.0"/>
  </goals>
  <goals id="B2" name="Sub-Goal 2" description="The AC ECU does not power the actuator if the vehicle speed is greater than 15 km/h." supports="//@strategies.0/@supportedBy.1" state="True" asil="B">
